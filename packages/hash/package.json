{
  "name": "@thumpnail/hash",
  "version": "0.0.0",
  "description": "Encoder and decoder for the Wolt BlurHash algorithm.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/index.js"
      },
      "import": "./dist/index.mjs",
      "module": "./dist/esm/index.js",
      "node": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "demo": "webpack-dev-server --open",
    "dev": "tsup --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ts-loader": "^9.5.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
